                                        <SelectItem value="Fibers">{language === 'hi' ? 'рд░реЗрд╢рдо' : language === 'ta' ? 'роЗро┤рпИ' : language === 'te' ? 'р░лр▒Ир░мр░░р▒Нр░▓р▒Б' : language === 'bn' ? 'ржлрж╛ржЗржмрж╛рж░' : language === 'gu' ? 'рк░рлЗрк╢рко' : language === 'mr' ? 'рдлрд╛рдпрдмрд░' : language === 'pa' ? 'ри░рйЗри╕ри╝рио' : 'Fibers'}</SelectItem>
